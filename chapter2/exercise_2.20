(import (chibi))
(import (scheme red))
(define (same-parity . l)
  (define (is-even? x) (= (modulo x 2) 0 ))
  (define (not-empty-list? x) (not (null? x)) )
  (filter not-empty-list? (if (is-even? (car l)) (map (lambda (x) (if (is-even? x) x '() )) l) (map (lambda (x) (if (not (is-even? x)) x '())) l)))
)

(display "Odds")
(newline)
(display (same-parity 1 2 3 4 5 6 7 8 9))
(newline)

(display "Evens")
(newline)
(display (same-parity 2 3 4 5 6 7 8 9))
(newline)
