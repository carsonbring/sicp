(import (chibi))

(define (square x) (* x x))
(define (square-list items)
  (if (null? items)
    '()
    (cons (square (car items)) (square-list (cdr items)))
  )
)
(define (map-square-list x)
  (map square x)
)
(display (square-list (list 1 2 3 4 5 6)))
(display (map-square-list (list 1 2 3 4 5 6)))
